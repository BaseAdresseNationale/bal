{
  "name": "@etalab/bal",
  "version": "1.0.0-1",
  "description": "Outils permettant de manipuler les bases Adresse locales",
  "repository": "git@github.com:etalab/bal.git",
  "author": "Ã‰quipe Adresse <adresse@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "bin",
    "lib",
    "browser",
    "*.js"
  ],
  "bin": {
    "bal": "bin/bal"
  },
  "scripts": {
    "lint": "xo",
    "build-minicog": "node scripts/build-minicog",
    "build": "pkg --targets node12-linux-x64,node12-macos-x64,node12-win-x64 .",
    "transpile": "rm -rf browser && babel lib --out-dir browser",
    "test": "nyc ava",
    "prepublishOnly": "yarn build-minicog && yarn transpile"
  },
  "dependencies": {
    "blob-to-buffer": "^1.2.9",
    "chalk": "^4.1.0",
    "date-fns": "^2.16.1",
    "iconv-lite": "^0.6.2",
    "jschardet-french": "^0.0.1",
    "lodash": "^4.17.20",
    "papaparse": "^5.3.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@etalab/decoupage-administratif": "^0.8.0",
    "ava": "^3.15.0",
    "codecov": "^3.8.1",
    "fs-extra": "^9.0.1",
    "nyc": "^15.1.0",
    "pkg": "^4.4.9",
    "xo": "^0.36.1"
  },
  "engines": {
    "node": ">= 12"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "unicorn/prefer-string-slice": "off",
      "unicorn/import-style": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-reduce": "off"
    }
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "browser": {
    "./lib/index.js": "./browser/index.js",
    "./browser/validate/index.js": "./browser/validate/parse/blob.js"
  },
  "browserslist": [
    "> 1% in FR",
    "last 2 versions",
    "Firefox ESR"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2,
          "helpers": true,
          "regenerator": true
        }
      ]
    ],
    "ignore": [
      "**/__tests__",
      "**/commands"
    ]
  }
}
