{
  "name": "@etalab/bal",
  "version": "0.20.4",
  "description": "Outils permettant de manipuler les bases Adresse locales",
  "repository": "git@github.com:etalab/bal.git",
  "author": "Ã‰quipe Adresse <adresse@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "bin",
    "lib",
    "browser",
    "*.js"
  ],
  "bin": {
    "bal": "bin/bal"
  },
  "scripts": {
    "lint": "xo",
    "build": "pkg --targets node12-linux-x64,node12-macos-x64,node12-win-x64 .",
    "transpile": "rm -rf browser && babel lib --out-dir browser",
    "test": "nyc ava",
    "prepublishOnly": "npm run transpile"
  },
  "dependencies": {
    "blob-to-buffer": "^1.2.8",
    "date-fns": "^2.9.0",
    "debug": "^4.1.1",
    "jschardet-french": "^0.0.1",
    "lodash": "^4.17.15",
    "papaparse": "^5.1.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "ava": "^2.4.0",
    "codecov": "^3.6.1",
    "nyc": "^15.0.0",
    "pkg": "^4.4.2",
    "xo": "^0.25.3"
  },
  "engines": {
    "node": ">= 8"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "unicorn/prefer-string-slice": "off"
    }
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "browser": {
    "./lib/index.js": "./browser/index.js"
  },
  "browserslist": [
    "> 1% in FR",
    "last 2 versions",
    "ie 11",
    "Firefox ESR"
  ],
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false,
        "configFile": false
      }
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2,
          "helpers": true,
          "regenerator": true
        }
      ]
    ]
  }
}
