{
  "name": "@etalab/bal",
  "version": "0.3.2",
  "description": "Outils permettant de manipuler les bases Adresse locales",
  "repository": "git@github.com:etalab/outils-bal.git",
  "author": "Team Geo <geo@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "bin",
    "lib",
    "browser",
    "*.js"
  ],
  "bin": {
    "bal": "bin/bal"
  },
  "scripts": {
    "lint": "xo",
    "build": "pkg .",
    "transpile": "babel lib --out-dir browser",
    "test": "ava"
  },
  "dependencies": {
    "blob-to-buffer": "^1.2.6",
    "debug": "^3.1.0",
    "jschardet-french": "^0.0.1",
    "luxon": "^0.2.5",
    "papaparse": "^4.3.6",
    "proj4": "^2.4.4",
    "yargs": "^10.0.3"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "pkg": "^4.3.0-beta.1",
    "xo": "^0.18.2"
  },
  "engines": {
    "node": ">= 8"
  },
  "xo": {
    "space": 2,
    "semicolon": false
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "browser": {
    "./lib/index.js": "./browser/index.js"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1% in FR",
              "last 2 versions",
              "ie 11",
              "Firefox ESR"
            ],
            "uglify": true
          }
        }
      ]
    ]
  }
}
