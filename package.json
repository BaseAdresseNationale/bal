{
  "name": "@etalab/bal",
  "version": "1.7.1",
  "description": "Outils permettant de manipuler les bases Adresse locales",
  "repository": "https://github.com/etalab/bal",
  "author": "Ã‰quipe Adresse <adresse@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "./lib/index.js",
  "files": [
    "bin",
    "lib",
    "browser",
    "minicog.json",
    "*.js"
  ],
  "bin": {
    "bal": "bin/bal"
  },
  "scripts": {
    "lint": "xo",
    "build-minicog": "node scripts/build-minicog",
    "build": "pkg --targets node14-linux-x64,node14-macos-x64,node14-win-x64 .",
    "transpile": "rm -rf browser && babel lib --out-dir browser",
    "test": "nyc ava",
    "prepublishOnly": "yarn build-minicog && yarn transpile"
  },
  "dependencies": {
    "blob-to-buffer": "^1.2.9",
    "chalk": "^4.1.2",
    "chardet": "^1.4.0",
    "date-fns": "^2.26.0",
    "file-type": "^12.4.2",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "papaparse": "^5.3.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@etalab/decoupage-administratif": "0.9.0-0",
    "ava": "^3.15.0",
    "codecov": "^3.8.3",
    "fs-extra": "^10.0.0",
    "nyc": "^15.1.0",
    "pkg": "^5.4.1",
    "xo": "^0.46.4"
  },
  "engines": {
    "node": ">= 14"
  },
  "xo": {
    "space": 2,
    "semicolon": false,
    "rules": {
      "comma-dangle": "off",
      "unicorn/prefer-string-slice": "off",
      "unicorn/import-style": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/no-reduce": "off",
      "unicorn/prefer-module": "off",
      "import/extensions": "off",
      "node/prefer-global/process": "off"
    }
  },
  "pkg": {
    "scripts": "lib/**/*.js"
  },
  "browser": {
    "./lib/index.js": "./browser/index.js",
    "./browser/validate/parse/index.js": "./browser/validate/parse/blob.js"
  },
  "browserslist": [
    "> 1% in FR",
    "last 2 versions",
    "Firefox ESR"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ]
    ],
    "ignore": [
      "**/__tests__",
      "**/commands"
    ]
  }
}
